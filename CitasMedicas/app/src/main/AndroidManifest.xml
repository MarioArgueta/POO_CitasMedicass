<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    package="com.example.citasmedicas">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <dist:module dist:instant="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Register_Perfil"
            android:label="@string/title_activity_register__perfil"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".RegirtrarCitax"
            android:label="@string/title_activity_regirtrar_citax"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Anular_Cita"
            android:label="@string/title_activity_anular__cita"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".registrarCita"
            android:label="@string/title_activity_registrar_cita"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_loginx"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".register_user"
            android:label="@string/title_activity_register_user"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Menu_Userx"
            android:label="@string/title_activity_menu__userx"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/title_activity_login" />
        <activity android:name=".loginx">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>